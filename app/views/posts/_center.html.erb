<div class="container text-center">    
    <div class="row">
        <ul> 
            <%@posts.each do |post|%>
                <% unless post.archived?%>
                    <%=render "postcard" , post: post%>
                <%end%>
                <br>
            <%end%>
        </ul>
    </div>
</div>